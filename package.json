{
  "name": "apex-dml-mocking",
  "version": "1.0.2",
  "description": "Easy DML / SOQL mocking through the Factory pattern in Apex",
  "dependencies": {},
  "devDependencies": {
    "@salesforce/cli": "2.105.6",
    "prettier-plugin-apex": "2.2.6",
    "prettier": "3.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamessimone/apex-dml-mocking.git"
  },
  "scripts": {
    "prettier": "prettier --write \"**/*.{cls,cmp,component,css,html,js,json,md,page,trigger,yaml,yml}\"",
